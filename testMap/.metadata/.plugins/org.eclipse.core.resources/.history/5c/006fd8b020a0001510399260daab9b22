package demos;

import processing.core.PApplet;
import processing.core.PImage;

public class myApplet extends PApplet {
	
	PImage img;
	public void setup()
	{
		
		img = loadImage("palmTrees.jpg", "jpg");
		size(img.width, img.height);
		//img.resize(0, height);
		//background(img);
		
	}
	
	public void draw()
	{
		img.resize(0, height);
		image(img, 0, 0);
	}

}
